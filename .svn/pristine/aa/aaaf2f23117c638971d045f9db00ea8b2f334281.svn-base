package com.zhongan.icare.message.push.enums;

public enum PushGroupEnum
{
    GROUP_TYPE_1(1, "跳转"),

    GROUP_TYPE_0(0, "普通"),

    GROUP_STATUS_1(1, "不可用"),

    GROUP_STATUS_0(0, "可用");

    private int code;
    private String des;

    private PushGroupEnum(int code, String des)
    {
        this.code = code;
        this.des = des;
    }

    public int getCode()
    {
        return code;
    }

    public String getDes()
    {
        return des;
    }

}
