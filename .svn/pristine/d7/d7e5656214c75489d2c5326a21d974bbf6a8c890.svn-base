package com.zhongan.icare.message.push.enums;

public enum PushTypeEnum
{

    ONLY_PUSH_TYPE_6(6, "推送单条给单个人"),

    PUSH_TYPE_5(5, "发送多条给多人"),

    PUSH_TYPE_4(4, "发送单条给所有人"),

    PUSH_TYPE_3(3, "发送单条给多人"),

    PUSH_TYPE_2(2, "发送单条给第三方"),

    PUSH_TYPE_1(1, "发送单条给单个人");

    private int code;
    private String des;

    private PushTypeEnum(int code, String des)
    {
        this.code = code;
        this.des = des;
    }

    public int getCode()
    {
        return code;
    }

    public String getDes()
    {
        return des;
    }

}
